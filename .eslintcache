 let cartItems = [];\r\n\r\n    Object.keys(items).map(id => {\r\n        Object.keys(items[id]).map(obj => {\r\n            // console.log(items[id][obj])\r\n            cartItems.push(items[id][obj])\r\n        })\r\n    });\r\n\r\n    // console.log(cartItems)\r\n\r\n    const onClearCart = () => {\r\n        if (window.confirm('Vai jūs tiešām gribāt dzēst grozu?')) {\r\n            dispatch(clearCart());\r\n        }\r\n    }\r\n\r\n    const onRemoveItem = (obj) => {\r\n        dispatch(removeItem(obj));\r\n    }\r\n\r\n    const onMinusItem = (obj) => {\r\n        dispatch(minusItem(obj));\r\n    }\r\n\r\n    const onPlusItem = (obj) => {\r\n        dispatch(plusItem(obj));\r\n    }\r\n\r\n    const checkout = () => {\r\n        // console.log(\"Pasūtījums, summa - \", totalPrice, items)\r\n        let finalCart = {\r\n            items: [],\r\n            totalPrice: totalPrice,\r\n            totalCount: totalCount\r\n        }\r\n\r\n        Object.keys(items).map((id) => {\r\n            let itemsID = Object.keys(items[id]);\r\n            itemsID.pop();\r\n            itemsID.map((param) => {\r\n                console.log(items[id][param])\r\n                finalCart.items.push(items[id][param]);\r\n            })\r\n        })\r\n        console.log(finalCart)\r\n    }\r\n\r\n    // console.log(cartItems)\r\n\r\n    return (\r\n        <div>\r\n            <Helmet>\r\n                <title>Grozs</title>\r\n            </Helmet>\r\n            <Header activeIndex={null}/>\r\n            <div className=\"content\">\r\n                { cartItems.length > 0 ?\r\n                <div className=\"container container--cart\">\r\n                    <div className=\"cart\">\r\n                        <div className=\"cart__top\">\r\n                            <h2 className=\"content__title\">\r\n                                <svg width=\"18\" height=\"18\" viewBox=\"0 0 18 18\" fill=\"none\"\r\n                                     xmlns=\"http://www.w3.or