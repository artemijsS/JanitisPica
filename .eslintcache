enas', type: 'price[0]', order: 'asc' },\r\n    { name: 'alfabēta', type: 'name', order: 'asc' },\r\n];\r\n\r\nfunction HomePage () {\r\n    const dispatch = useDispatch();\r\n    const pizzas = useSelector(({ pizzas }) => pizzas.items);\r\n    const cart = useSelector(({ cart }) => cart.items);\r\n    const isLoaded = useSelector(({ pizzas }) => pizzas.isLoaded);\r\n    const { category, sortBy } = useSelector(({ filters }) => filters);\r\n\r\n    useEffect(() => {\r\n        dispatch(fetchPizzas(category,sortBy))\r\n    }, [category, sortBy]);\r\n\r\n\r\n    const onSelectCategory = useCallback(index => {\r\n        dispatch(setCategory(index));\r\n    }, []);\r\n\r\n    const onSelectSortType = useCallback((type) => {\r\n        dispatch(setSortBy(type));\r\n    }, []);\r\n\r\n    const clickedOnAddPizza = (obj) => {\r\n        dispatch(addItemToCart(obj));\r\n    }\r\n\r\n\r\n    return (\r\n        <div>\r\n            <Helmet>\r\n                <title>Picas</title>\r\n            </Helmet>\r\n            <Header activeIndex={0} />\r\n            <div className=\"content\">\r\n                <div className=\"container\">\r\n                    <div className=\"content__top\">\r\n                        <Categories\r\n                            activeCategory={category}\r\n                            onClickItem={onSelectCategory}\r\n                            items={categoryNames}\r\n                        />\r\n                        <SortPopUp\r\n                            activeSort={sortBy.type}\r\n                            onClickItem={onSelectSortType}\r\n                            items={sortItems}\r\n                        />\r\n                    </div>\r\n                    <h2 className=\"content__title\">Visas picas</h2>\r\n                    <div className=\"content__items\">\r\n                        {isLoaded\r\n                            ? pizzas.map((pizza) => (\r\n                                    <PizzaBlock\r\n             